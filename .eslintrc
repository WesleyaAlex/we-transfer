{
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest": true
  },
  "extends": [
    "airbnb"
  ],
  "plugins": [
    "react",
    "import-helpers",
    "react-hooks",
    "jsx-runtime"
  ],
  "rules": {
    "no-tabs": 0,
    "linebreak-style": 0,
    "multiline-ternary": [
      "error",
      "always"
    ],
    "curly": "error",
    "class-methods-use-this": 0,
    "comma-dangle": 0,
    "no-param-reassign": 0,
    "no-use-before-define": 0,
    "padded-blocks": 0,
    "quote-props": 0,
    "semi": [
      0,
      "never"
    ],
    "template-curly-spacing": [
      2,
      "always"
    ],
    "react/forbid-prop-types": 0,
    "react-hooks/rules-of-hooks": "error",
    "react-hooks/exhaustive-deps": 0,
    "react/jsx-curly-spacing": [
      2,
      "always"
    ],
    "react/jsx-filename-extension": 0,
    "react/no-unused-prop-types": 0,
    "react/prop-types": [
      1,
      {
        "ignore": [
          "value",
          "original",
          "item"
        ]
      }
    ],
    "import/prefer-default-export": 0,
    "import/first": 0,
    "react/no-array-index-key": 0,
    "react/prefer-stateless-function": 0,
    "react/jsx-no-bind": 0,
    "no-plusplus": "off",
    "import/no-extraneous-dependencies": 0,
    "jsx-a11y/click-events-have-key-events": "off",
    "import-helpers/order-imports": [
      "warn",
      {
        "newlinesBetween": "always",
        "groups": [
          "/^react/",
          "module",
          "/^@/",
          [
            "parent",
            "sibling",
            "index"
          ],
          "/^./styles/"
        ],
        "alphabetize": {
          "order": "asc",
          "ignoreCase": true
        }
      }
    ]
  },
  "overrides": [
    {
      "files": [
        "**/*.tsx"
      ],
      "rules": {
        "react/prop-types": 0
      }
    }
  ],
  "parserOptions": {
    "ecmaVersion": 2020
  }
}